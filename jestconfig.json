{
  "preset": "ts-jest",
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "coverageReporters": ["text", "json-summary", "lcov"], 
  "collectCoverageFrom": ["src/**/*.{ts,js}"]
}
